<keymap version="1" name="Windows Proper Redo" parent="$default">
  <action id="$Redo">
    <keyboard-shortcut first-keystroke="shift ctrl z" />
    <keyboard-shortcut first-keystroke="shift alt back_space" />
    <keyboard-shortcut first-keystroke="ctrl y" />
  </action>
  <action id="ActivateFavoritesToolWindow" />
  <action id="ActivateFindToolWindow" />
  <action id="ActivateRunToolWindow">
    <keyboard-shortcut first-keystroke="alt 3" />
  </action>
  <action id="ActivateStructureToolWindow">
    <keyboard-shortcut first-keystroke="alt 4" />
  </action>
  <action id="ActivateTODOToolWindow">
    <keyboard-shortcut first-keystroke="alt 2" />
  </action>
  <action id="ActivateTerminalToolWindow" />
  <action id="EditorDeleteLine">
    <keyboard-shortcut first-keystroke="ctrl l" />
  </action>
  <action id="FindNext">
    <keyboard-shortcut first-keystroke="f3" />
  </action>
  <action id="Terminal.ClearBuffer" />
  <action id="ToggleFullScreen">
    <keyboard-shortcut first-keystroke="f10" />
  </action>
  <action id="ToggleZenMode">
    <keyboard-shortcut first-keystroke="alt dead_circumflex" />
  </action>
  <action id="Vcs.Log.FocusTextFilter" />
</keymap>